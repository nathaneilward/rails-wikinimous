<h1>Show Article!</h1>
<h2><%= @article.id %></h2>
<h2><%= @article.title %></h2>
<h2><%= @article.content %></h2>
<%= link_to "Index", articles_path(@article)%>
<%= link_to "Edit", edit_article_path(@article)%>
<%= link_to "Delete", article_path(@article),
      method: :delete,
      data: { confirm: "Are you sure?" } %>


