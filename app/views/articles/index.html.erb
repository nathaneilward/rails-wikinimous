<h1>Index!</h1>


<% @articles.each do |article| %>
<h2><%= article.id %></h2>
<h2><%= article.title %></h2>
<%= link_to "Delete", article_path(article),
      method: :delete,
      data: { confirm: "Are you sure?" } %>
<%= link_to "Go to article", article_path(article)%>
<%= link_to "Create", new_article_path(article)%>
<%= link_to "Edit", edit_article_path(article)%>
<% end %>


